<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.akhudoyarova.entities.LoginhashEntity" table="loginhash" schema="public" catalog="postgres">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="hash">
            <column name="hash" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="login">
            <column name="login" sql-type="varchar" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>